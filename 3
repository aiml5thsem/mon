import wrapt

@wrapt.decorator
def log_convert(wrapped, instance, args, kwargs):
    event = args[0] if args else kwargs.get("event")
    print("convert_log_event called with event:", event)
    return wrapped(*args, **kwargs)

from mcp_agent.event_progress import convert_log_event
convert_log_event = log_convert(convert_log_event)
